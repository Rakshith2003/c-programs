#include<stdio.h>
#include<string.h>
int palin(char *str,int len)
{
int mid=len/2;
    for(int i=0;i<mid;i++)
{
        if(str[i]!=str[len-i-1])
{
            return 0;
 }
    }
return 1;
}
void main()
{
char str1[]="malayalam";
    int len=strlen(str1);
if(palin(str1,len))
    {
printf("its a palidrome ");
    }
else
        {
printf("not a palindrome");
        }
}
